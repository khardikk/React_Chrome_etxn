{
    "manifest_version": 3,
    "name": "React Color changer",
    "version": "1.0.0",
    "description": "Change the color of the page",
    "action": {
        "default_popup": "index.html"
     
    },
    "permissions": [
        "scripting"
    ],
    "host_permissions": [
        "http://*/*",
        "https://*/*"
    ]
}